<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.t.inotes">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/inotes"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.INotes">
        <activity android:name=".starting_screen" />
        <activity
            android:name=".nav_code"
            android:label="Source Code 🔥"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".playmusic"
            android:label="iMusic 🎶❤"
            android:parentActivityName=".music" />
        <activity
            android:name=".music"
            android:label="iMusic 🎶"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity1"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.INotes.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>